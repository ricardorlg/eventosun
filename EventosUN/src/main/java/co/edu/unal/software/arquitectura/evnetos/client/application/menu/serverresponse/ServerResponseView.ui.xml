<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.popup {
		background-color: white;
		border: 3px double black;
		padding: 0 15px;
	}
	</ui:style>
	<g:DecoratedPopupPanel
		autoHideOnHistoryEventsEnabled="true" autoHideEnabled="true" width="400"
		height="400" styleName="{style.popup}">
		<g:SimplePanel width="100%" height="100%">
			<g:HTML ui:field="response" wordWrap="true" width="100%"
				height="100%"></g:HTML>
		</g:SimplePanel>

	</g:DecoratedPopupPanel>
</ui:UiBinder>
