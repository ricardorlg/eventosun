<?xml version='1.0' encoding='UTF-8'?>
<module rename-to='eventos'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='com.google.gwt.inject.Inject' />

	<!-- GXT -->
	<stylesheet src="reset.css" />
	<inherits name='com.sencha.gxt.ui.GXT' />



	<!-- GXT Theme -->
	<!-- <inherits name='com.sencha.gxt.theme.blue.Blue'/> -->
	<inherits name='com.sencha.gxt.theme.gray.Gray' />

	<!-- GWTP -->
	<inherits name='com.gwtplatform.dispatch.Dispatch' />
	<inherits name='com.gwtplatform.mvp.MvpWithEntryPoint' />
	<inherits name='com.rhemsolutions.PopupWindow' />
	<inherits name='com.bradrydzewski.gwt.calendar.Calendar' />
	<inherits name='com.bradrydzewski.gwt.calendar.theme.google.Google' />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />

	<source path='client' />
	<source path='shared' />
	<extend-property name='locale' values='en' />
	<set-property-fallback name='locale' value='en' />
	<set-property name='locale' value='en' />
	<!-- Default GIN Module -->
	<set-configuration-property name="gin.ginjector.modules"
		value="co.edu.unal.software.arquitectura.evnetos.client.gin.ClientModule" />
	<!-- Bootstrapper -->
	<set-configuration-property name="gwtp.bootstrapper"
		value="co.edu.unal.software.arquitectura.evnetos.client.gin.BootstrapperImpl" />
</module>
